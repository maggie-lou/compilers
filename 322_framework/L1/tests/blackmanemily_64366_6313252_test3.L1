(:main
	(:main
	0 0
	rdi <- 1
	mem rsp -8 <- :re1
	call :encode 1
	:re1
	rdi <- rax
	call print 1

	rdi <- 2
	mem rsp -8 <- :re2
	call :encode 1
	:re2
	rdi <- rax
	call print 1

	rdi <- 51
	mem rsp -8 <- :re3
	call :encode 1
	:re3
	rdi <- rax
	call print 1

	rdi <- -10
	mem rsp -8 <- :re4
	call :encode 1
	:re4
	rdi <- rax
	call print 1

	return)

	// encodes a number
	(:encode
	1 1
	rdi <<= 1
	rdi += 1
	mem rsp 0 <- rdi
	call print 1  //should print encoded number
	rdi <- mem rsp 0
	rax <- rdi
	return
	)
)

//expected output:
// 1
// 1
// 2
// 2
// 52
// 52
// -10
// -10