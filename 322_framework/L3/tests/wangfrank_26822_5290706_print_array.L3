    define :encode (%data) {
        %data <- %data << 1
        %data <- %data + 1
        return %data
    }

    define :print_array (%vec_ptr) {
        %size <- load %vec_ptr
        %size_e <- call :encode(%size)
        call print(%size_e)
        %ptr <- %vec_ptr
        
        :loop
        %ptr < %ptr + 8
        %over <- %size = 0
        br %over :finish
        br :cont

        :cont
        %ele <- load %ptr
        call print (%ele)
        %size <- %size - 1
        br :loop

        :finish
        return
    }

