// 9 factorial
(:go
	(:go
		0 0
		%number <- 9 // number = 9
		%multiplier <- %number 
		:loop
		%multiplier -= 1 // decrement 
		%number *= %multiplier // multiply number by the decremented number
		r13 <- %multiplier
		cjump r13 = 1 :done :loop
		:done
		rdi <- %number
		rdi <<= 1
		rdi += 1
		call print 1
		return
	)
)
