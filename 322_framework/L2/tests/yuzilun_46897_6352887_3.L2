// return the first element in array[6,9,10,15,11] that's greater than 10
// if such an element doesn't exist, return the last element in the array
(:go
 (:go
  0 0
  rdi <- 9
  rsi <- 0
  call allocate 2
  mem rax 8 <- 13
  mem rax 16 <- 19
  mem rax 24 <- 21
  mem rax 32 <- 31
  mem rax 40 <- 23
  mem rsp -8 <- :f_ret
  rdi <- rax
  call :f 1
  :f_ret
  rdi <- rax
  call print 1
  return
 )
 (:f
  1 0
  // r10 stores the index of the current array element 
  // r8 stores the total number of elements in array 
  %index <- 0
  %length <- mem rdi 0

  :loop
  cjump %length <= %index :break :continue

  :continue 
  rdi += 8
  rax <- mem rdi 0
  cjump 21 < rax :break :count

  :count
  %index += 1
  goto :loop

  :break
  return
 )
)