(:main
	(:main
		0 0
		%start <- 3
		%sum <- 1
		goto :square
		:outer
		%sum -= 1
		%sum += %square
		%start += 2
		// Print the most recent square
		rdi <- %square
		call print 1
		// Now print the running sum
		rdi <- %sum
		call print 1
		// And finally print 'nil' to indicate that one block is done
		rdi <- 0
		call print 1
		// Continue until after 100
		cjump %start = 203 :end :square
		:end
		return
		
		:square
		// Print the currently squaring number
		rdi <- %start
		call print 1
		%square <- %start
		// Strip encoding and square
		%square -= 1
		%square >>= 1
		%square *= %square
		// Then reencode
		%square <<= 1
		%square += 1
		goto :outer
	)
)